{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "import/no-extraneous-dependencies": ["error", {"packageDir": "./"}],
      "react/prop-types": 0, //WE WILL FIX LATER
      "react/require-default-props": 0, //WE WILL FIX LATER
      "react/forbid-prop-types":0, //WE WILL FIX LATER
      "import/prefer-default-export":0, //WE WILL FIX LATER
      "react/state-in-constructor": 0,
      "react/destructuring-assignment":0,
      "react/jsx-no-bind":0,
      "react/jsx-props-no-spreading":0,
      "semi": 0,   
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
  }
  